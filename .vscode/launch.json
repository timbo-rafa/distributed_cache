    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "concurrency.py",
            "type": "python",
            "request": "launch",
            "program": "examples/concurrency.py",
            "console": "integratedTerminal"
        },
        {
            "name": "replication.py",
            "type": "python",
            "request": "launch",
            "program": "examples/replication.py",
            "console": "integratedTerminal"
        },
        {
            "name": "cache_api.py",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "backend/api-container/cache_api.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0",
                "CA_CLUSTER_IP": "172.17.0.2",
                "BR_CLUSTER_IP": "172.17.0.4",
                "API_IP": "172.17.0.1"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
                "--host",
                "0.0.0.0"
            ],
            "jinja": true
        }
    ]
}